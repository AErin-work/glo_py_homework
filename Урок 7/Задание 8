n = int(input())
maximum = 0
minimum = 999999999999999999999999999999999999999999999999999
if n >= 2:
    for i in range(n):
        m = int(input())
        if m > maximum:
            maximum = m
            if m < minimum:
                minimum = m
    print(f'Минимум равен {minimum}')
    print(f'Максимум равен {maximum}')
else:
    print('Введи цифру больше или равную 2')
